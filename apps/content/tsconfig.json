{
    "extends": "@cohbrgr/tsconfig/app",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "src/*": ["./src/*"],
            "@cohbrgr/content/*": ["../content/*"]
        },
        "typeRoots": [
            "../../node_modules/@types",
            "./node_modules/@types",
            "./src/client/@types/*"
        ]
    },
    "exclude": ["../../node_modules", "node_modules"]
}

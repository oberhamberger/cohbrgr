{
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{workspaceRoot}/dist",
        "{workspaceRoot}/bin"
      ]
    },
    "start": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{workspaceRoot}/dist",
        "{workspaceRoot}/bin"
      ]
    }
  }
}

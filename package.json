    "keywords": [
        "express",
        "typescript"
    ],
    "scripts": {
        "lint": "eslint .eslintrc.js --ext .ts src --fix",
        "prebuild": "rm -rf ./dist && mkdir ./dist && npm run lint && cp -r ./static/ ./dist/public",

        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "helmet": "^7.0.0",
        "nocache": "^4.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^6.14.0",
        "react-router-dom": "^6.14.0",
        "winston": "^3.9.0",

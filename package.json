{
    "name": "cohbrgr",
    "version": "1.0.0",
    "main": "dist/server",
    "description": "temporary website gone mad",
    "private": true,
    "keywords": [
        "express",
        "typescript",
        "react",
        "css-modules",
        "ssr"
    ],
    "scripts": {
        "lint": "eslint .eslintrc.js src --fix",
        "prestart": "rm -rf ./dist && npm run lint",
        "prebuild": "rm -rf ./dist && npm run lint",
        "build": "cross-env TS_NODE_PROJECT=\"build/tsconfig.json\" NODE_ENV=production webpack --config build/index.ts",
        "analyze": "cross-env TS_NODE_PROJECT=\"build/tsconfig.json\" NODE_ENV=production ANALYZE=true webpack --config build/index.ts",
        "start": "cross-env TS_NODE_PROJECT=\"build/tsconfig.json\" NODE_ENV=development webpack --watch --config build/index.ts",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oberhamberger/cohbrgr.git"
    },

{
    "name": "cohbrgr",
    "version": "1.0.0",
    "main": "dist/server",
    "description": "temporary website gone mad",
    "private": true,
    "keywords": [
        "express",
        "typescript",
        "react",
        "css-modules",
        "ssr"
    ],
    "scripts": {
        "cleanup": "rm -rf ./dist",
        "bootstrap": "rm -rf ./node_modules && npm run cleanup && npm install",
        "lint": "eslint .eslintrc.js build && eslint .eslintrc.js src",
        "prestart": "npm run cleanup && npm run lint",
        "prebuild": "npm run cleanup",
        "build": "cross-env TS_NODE_PROJECT=\"build/tsconfig.json\" NODE_ENV=production webpack --config build/index.ts",
        "build:analyze": "cross-env TS_NODE_PROJECT=\"build/tsconfig.json\" NODE_ENV=production ANALYZE=true webpack --config build/index.ts",
        "start": "cross-env TS_NODE_PROJECT=\"build/tsconfig.json\" NODE_ENV=development webpack --watch --config build/index.ts",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oberhamberger/cohbrgr.git"
    },
    "author": "Christian Oberhamberger",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/oberhamberger/cohbrgr/issues"
    },
    "homepage": "https://github.com/oberhamberger/cohbrgr#readme",
    "dependencies": {
        "compression": "^1.7.4",
        "express": "^4.17.3",
        "express-rate-limit": "^6.3.0",
        "helmet": "^5.0.2",
        "nocache": "^3.0.3",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-router": "^6.2.2",
        "react-router-dom": "^6.2.2",
        "winston": "^3.6.0",
        "workbox-core": "^6.5.2",
        "workbox-expiration": "^6.5.2",
        "workbox-navigation-preload": "^6.5.2",
        "workbox-routing": "^6.5.2",
        "workbox-strategies": "^6.5.2",
        "workbox-window": "^6.5.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.3",
        "@testing-library/react": "^12.1.4",
        "@tsconfig/node16": "^1.0.2",
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.23",
        "@types/react": "^17.0.43",
        "@types/react-dom": "^17.0.14",
        "@types/react-router": "^5.1.18",
        "@types/react-router-dom": "^5.3.3",
        "@types/webpack": "^5.28.0",
        "@types/webpack-bundle-analyzer": "^4.4.1",
        "@types/webpack-env": "^1.16.3",
        "@types/workbox-webpack-plugin": "^5.1.8",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "copy-webpack-plugin": "^10.2.4",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jsdoc": "^38.1.4",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-webpack-plugin": "^3.1.1",
        "html-validator": "^6.0.0",
        "jest": "^27.5.1",
        "jest-transform-stub": "^2.0.0",
        "mini-css-extract-plugin": "^2.6.0",
        "node-mocks-http": "^1.11.0",
        "node-sass": "^7.0.1",
        "nodemon-webpack-plugin": "^4.7.1",
        "prettier": "2.6.1",
        "raw-loader": "^4.0.2",
        "sass": "^1.49.9",
        "sass-loader": "^12.6.0",
        "ts-jest": "^27.1.4",
        "ts-loader": "^9.2.8",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3",
        "webpack": "^5.70.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.2",
        "webpackbar": "^5.0.2"
    }
}

        "jest-environment-jsdom": "^29.5.0",
        "jest-transform-stub": "^2.0.0",
        "mini-css-extract-plugin": "^2.7.6",
        "node-mocks-http": "^1.12.2",
        "nodemon-webpack-plugin": "^4.8.1",
        "prettier": "^2.8.8",

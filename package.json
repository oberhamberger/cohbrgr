        "prettier": "2.3.2",
        "ts-loader": "^9.2.5",
        "ts-node": "^10.2.0",
        "typescript": "^4.3.5",
        "webpack": "^5.50.0",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^3.11.2",

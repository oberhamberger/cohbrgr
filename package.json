        "@tsconfig/node20": "^1.0.2",
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.2",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
